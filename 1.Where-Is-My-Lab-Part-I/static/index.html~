<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>A Prototype Project</title>
    <script type="text/javascript" src="socketio.js"></script>
    <script>
      io.connect('http://localhost').on('refresh', function () {
        if(io.refreshApp) { window.location.reload(); }
        else { io.refreshApp = true; }
      });
    </script>
  </head>
  <body>
    a body!
  </body>
</html>
